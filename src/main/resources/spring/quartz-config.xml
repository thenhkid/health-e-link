     <schedule>
        <job>
            <name>ProcessInBoundBatches</name>
            <group>processGroup</group>
            <description>This process will run every 30 seconds on the 5 second of the minute  to check if any loaded files will need to be process</description>
            <job-class>com.ut.healthelink.jobs.processBatches</job-class>
        </job>
 
        <trigger>
            <cron>
                <name>ProcessInBoundBatchesTrigger</name>
                <job-name>ProcessInBoundBatches</job-name>
                <job-group>processGroup</job-group>
                <cron-expression>5/30 * * * * ?</cron-expression>
            </cron>
        </trigger>
    </schedule>
    
    <schedule>
        <job>
            <name>ProcessMassInBoundBatches</name>
            <group>processGroup</group>
            <description>This process will run every 6 </description>
            <job-class>com.ut.healthelink.jobs.processMassBatches</job-class>
        </job>
 
        <trigger>
            <cron>
                <name>ProcessMassInBoundBatchesTrigger</name>
                <job-name>ProcessMassInBoundBatches</job-name>
                <job-group>processGroup</job-group>
                <!-- <cron-expression>0 0/0 0,3,6,9,12,15,18,21 * * ?</cron-expression> -->
                 <cron-expression>0 0/6 * * * ?</cron-expression>
            </cron>
        </trigger>
    </schedule>
    
