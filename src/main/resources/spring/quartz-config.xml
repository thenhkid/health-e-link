<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data
    xmlns="http://www.quartz-scheduler.org/xml/JobSchedulingData"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.quartz-scheduler.org/xml/JobSchedulingData 
        http://www.quartz-scheduler.org/xml/job_scheduling_data_1_8.xsd"
    version="1.8">
 
   <schedule>
        <job>
            <name>ProcessOutputFiles</name>
            <group>processGroup</group>
            <description>This process will run every five minute to check if any outbound files must be processed</description>
            <job-class>com.ut.healthelink.jobs.processOutputRecords</job-class>
        </job>
 		<!--  changed to every 10 seconds because we are processing one by one -->
        <trigger>
            <cron>
                <name>ProcessOutputFilesTrigger</name>
                <job-name>ProcessOutputFiles</job-name>
                <job-group>processGroup</job-group>
                 <cron-expression>0/10 * * * * ?</cron-expression>
            </cron>
        </trigger>
    </schedule>
    <schedule>
        <job>
            <name>GenerateOutputFiles</name>
            <group>processGroup</group>
            <description>This process will look to see if any output files need to be generated runs every 5 minutes.</description>
            <job-class>com.ut.healthelink.jobs.generateOutputFiles</job-class>
        </job>
 
        <trigger>
            <cron>
                <name>GenerateOutputFilesTrigger</name>
                <job-name>GenerateOutputFiles</job-name>
                <job-group>processGroup</job-group>
                <cron-expression>0 0/5 * * * ?</cron-expression>
            </cron>
        </trigger>
    </schedule>
    
     <schedule>
        <job>
            <name>LoadBatches</name>
            <group>processGroup</group>
            <description>This process will run every 3o seconds starting at 10 second on the minute to check if any uploaded files need to be loaded</description>
            <job-class>com.ut.healthelink.jobs.loadBatches</job-class>
        </job>
 
        <trigger>
            <cron>
                <name>LoadBatchesTrigger</name>
                <job-name>LoadBatches</job-name>
                <job-group>processGroup</job-group>
                <cron-expression>10/30 * * * * ?</cron-expression>
            </cron>
        </trigger>
    </schedule>
    
         <schedule>
        <job>
            <name>LoadMassBatches</name>
            <group>processGroup</group>
            <description>This process will run every 5 minutes to look for large files</description>
            <job-class>com.ut.healthelink.jobs.loadMassBatches</job-class>
        </job>
 
        <trigger>
            <cron>
                <name>LoadMassBatchesTrigger</name>
                <job-name>LoadMassBatches</job-name>
                <job-group>processGroup</job-group>
                <!-- <cron-expression>0 15/0 1,4,7,10,13,16,19,22 * * ?</cron-expression> -->
                <cron-expression>0 0/5 * * * ?</cron-expression>
            </cron>
        </trigger>
    </schedule>
